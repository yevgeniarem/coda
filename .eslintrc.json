{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "object-curly-newline": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "operator-linebreak": 0,
    "camelcase": 0,
    "implicit-arrow-linebreak": 0,
    "react/jsx-one-expression-per-line": 0
  }
}
